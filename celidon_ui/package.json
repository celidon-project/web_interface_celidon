{
  "name": "celidon-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bulma": "^0.7.0",
    "leaflet": "^1.3.1",
    "socket.io": "^2.4.0",
    "typeface-source-code-pro": "^0.0.54",
    "typeface-source-sans-pro": "^0.0.54",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-alpha.5",
    "@vue/cli-plugin-eslint": "^3.0.0-alpha.5",
    "@vue/cli-service": "^3.0.0-alpha.5",
    "vue-template-compiler": "^2.5.13"
  },
  "vue": {
    "lintOnSave": true
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
